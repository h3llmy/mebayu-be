  "query": "\n            SELECT \n                p.*,\n                c.name as category_name,\n                c.created_at as category_created_at,\n                c.updated_at as category_updated_at\n            FROM products p\n            JOIN product_categories c ON p.category_id = c.id\n            WHERE p.id = $1\n            ",
